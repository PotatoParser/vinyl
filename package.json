{
  "name": "vinyl",
  "version": "1.0.0",
  "description": "Ad-free, open-source YouTube downloading website using ytdl-core & FFmpeg",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "build": "node-sass src/sass -o public/styles --recursive false & node ./build.js",
    "watch": "node-sass src/sass -wo public/styles",
    "dev": "node ./index.js",
    "now-build": "npm run build"
  },
  "author": "Wilson Nguyen",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "request": "^2.88.2",
    "ytdl-core": "^4.8.3",
    "ytpl": "^2.2.1"
  },
  "devDependencies": {
    "browserify": "^16.5.2",
    "loose-envify": "^1.4.0",
    "node-sass": "^4.14.1"
  }
}
